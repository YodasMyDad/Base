@model Barebone.ViewModels.Shared.Scripts.ScriptsViewModel
@if(Model.Scripts.Any())
{
    @section @Model.Scripts[0].Section
    {
        @foreach (var script in Model.Scripts)
        {
          @Html.Partial("_Script", script)
        }
    }
}